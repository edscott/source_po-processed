msgstr "Yeni UNIX şifrəsini yenidən yazın: "

msgid "Enter new UNIX password:"
msgstr "Yeni UNIX şifrəsini yazın: "

msgid "(current) UNIX password:"
msgstr "(hazırkı) UNIX şifrəsi: "

