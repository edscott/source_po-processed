msgid "KDE tool for opening URLs from the command line"
msgstr "Isixhobo se KDE sokuvula ii URL kwilayini yomyalelo"

msgid ""
"\n"
"Syntax:\n"
msgstr ""
"\n"
"Ubeko lwamagama ngokwezivakalisi:\n"

msgid ""
"  kfmclient openURL 'url' ['mimetype']\n"
"            # Opens a window showing 'url'.\n"
"            #  'url' may be a relative path\n"
"            #   or file name, such as . or subdir/\n"
"            #   If 'url' is omitted, $HOME is used instead.\n"
"\n"
msgstr ""
"  kfmclient openURL i-'url' ['minimedidi']\n"
"            # Ivula i window ebonisa i-'url'.\n"
"            #  'url' inokuba ngumendo ozalanayo\n"
"            #   okanye igama lefayile, njenge . okanye subdir/\n"
"            #   Ukuba 'url' ayiqukwanga, $HOME iyasetyenziswa endaweni.\n"
"\n"

msgid ""
"            # If 'mimetype' is specified, it will be used to determine the\n"
"            #   component that Konqueror should use. For instance, set it "
"to\n"
"            #   text/html for a web page, to make it appear faster\n"
"\n"
msgstr ""
"            # Ukuba 'mimedidi' ikhankanyiwe, izakusetyenziswa ukugqiba \n"
"            #   indawo i konqueror engayisebenzisa. Umzekelo, uyicwangcise "
"ku\n"
"            #   umbhalo/html yephepha le-web, ukuyenza ivele ngokukhawuleza\n"
"\n"

msgid ""
"  kfmclient openProfile 'profile' ['url']\n"
"            # Opens a window using the given profile.\n"
"            #   'profile' is a file under ~/.kde/share/apps/konqueror/"
"profiles.\n"
"            #   'url' is an optional URL to open.\n"
"\n"
msgstr ""
"  kfmclient vulaImboniselo yabucala 'imboniselo yabucala' ['url']\n"
"            # Ivula iwindow isebenzisa imboniselo yabucala enikiweyo.\n"
"            #   'imboniselo yabucala' yifayile phantsi ~/.kde/share/apps/"
"konqueror/iimboniselo zabucala.\n"
"            #   'url' yi-URL eyodwa ukuvula.\n"
"\n"

msgid "Profile %s not found\n"
msgstr "Imboniselo yabucala %s ayifumaneki\n"

msgid "Syntax Error: Not enough arguments\n"
msgstr "Imposiso yobeko lwamagama ngokwezivakalisi: Azanelanga iimpikiswana\n"

msgid "Syntax Error: Too many arguments\n"
msgstr ""
"Imposiso yobeko lwamagama ngokwezivakalisi: Kukho iimpikiswano ezininzi "
"kakhulu\n"

msgid "Syntax Error: Unknown command '%s'\n"
msgstr "Imposiso yobeko lwamagama ngokwezivakalisi: Umyalelo ongaziwayo '%s'\n"

msgid ""
"  kfmclient openProperties 'url'\n"
"            # Opens a properties menu\n"
"\n"
msgstr ""
"  kfmclient ivula Iiimpahla 'url'\n"
"            # Ivula imenu yeempahla\n"
"\n"

msgid ""
"  kfmclient exec ['url' ['binding']]\n"
"            # Tries to execute 'url'. 'url' may be a usual\n"
"            #   URL, this URL will be opened. You may omit\n"
"            #   'binding'. In this case the default binding\n"
msgstr ""
"  kfmclient exec ['url' ['bophelela']]\n"
"            # Izama ukuphumeza 'url'. 'url' inokubasisiqhelo\n"
"            #   URL, le URL izakuvulwa. Unokuquka \n"
"            #   'bophelela'. Kulo mba isibophelelo sendalo\n"

msgid ""
"            #   is tried. Of course URL may be the URL of a\n"
"            #   document, or it may be a *.desktop file.\n"
msgstr ""
"            #   iyazanywa. Kunjalo URL inokuba yi-URL ye\n"
"            #   umbhalo, okanye inokuba *.desktop yifayile.\n"

msgid ""
"            #   This way you could for example mount a device\n"
"            #   by passing 'Mount default' as binding to \n"
"            #   'cdrom.desktop'\n"
"\n"
msgstr ""
"            #   Ngale ndlela unokunyusa umzekelo icebo\n"
"            #   ngokugqithisa 'Unyuko olungagqibekanga' njengesibophelelo "
"kwi \n"
"            #   'cdrom.desktop'\n"
"\n"

msgid ""
"  kfmclient move 'src' 'dest'\n"
"            # Moves the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
msgstr ""
"  kfmclient shukumisa 'src' 'dest'\n"
"            # Ishukumisa i URL 'src' kwi 'dest'.\n"
"            #   'src' inokuba luluhlu lwee URL.\n"

msgid ""
"  kfmclient download ['src']\n"
"            # Copies the URL 'src' to a user specified location'.\n"
"            #   'src' may be a list of URLs, if not present then\n"
"            #   a URL will be requested.\n"
"\n"
msgstr ""
"  kfmclient layisha ezantsi ['src']\n"
"            # Ikhuphela i URL 'src' kwindawo yomsebenzisi ekhankanyiweyo "
"'.\n"
"            #   'src' inokuba luluhlu lwee URL, ukuba ayikho\n"
"            # i URL izakucelwa.\n"
"\n"

msgid ""
"  kfmclient copy 'src' 'dest'\n"
"            # Copies the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
"\n"
msgstr ""
"  kfmclient khuphela 'src' 'dest'\n"
"            # Ikhuphela i URL 'src' kwi 'dest'.\n"
"            #   'src' inokuba luluhlu lwee URL.\n"
"\n"

msgid ""
"  kfmclient sortDesktop\n"
"            # Rearranges all icons on the desktop.\n"
"\n"
msgstr ""
"  kfmclient beka kakuhleDesktop\n"
"           # Ibeka kakuhle zonke ii-Iconkwi desktop.\n"
"\n"

msgid ""
"  kfmclient configure\n"
"            # Re-read Konqueror's configuration.\n"
"\n"
msgstr ""
"  kfmclient qwalasela\n"
"            # Funda-kwakhona uqwalaselo lwe konqueror.\n"
"\n"

msgid ""
"  kfmclient configureDesktop\n"
"            # Re-read kdesktop's configuration.\n"
"\n"
msgstr ""
"  kfmclient qwalaselaDesktop\n"
"            # Funda-kwakhona uqwalaselo lwekdesktop.\n"
"\n"

msgid ""
"*** Examples:\n"
"  kfmclient exec file:/root/Desktop/cdrom.desktop \"Mount default\"\n"
"             // Mounts the CD-ROM\n"
"\n"
msgstr ""
"*** Imizekelo:\n"
"  kfmclient exec.fayile:/root/Desktop/cdrom.desktop \"Unyuko "
"olungagqibekanga\"\n"
"            // Inyusa i CDROM\n"
"\n"

msgid ""
"  kfmclient exec file:/home/weis/data/test.html\n"
"             // Opens the file with default binding\n"
"\n"
msgstr ""
"  kfmclient exec file:/home/weis/data/vavanya.html\n"
"            // Ivula ifayile ngobophelelo olungagqibekanga\n"
"\n"

msgid ""
"  kfmclient exec file:/home/weis/data/test.html Netscape\n"
"             // Opens the file with netscape\n"
"\n"
msgstr ""
"  kfmclient exec file:/home/weis/data/vavanya.html Netscape\n"
"            // Ivula ifayile nge netscape\n"
"\n"

msgid ""
"  kfmclient exec ftp://localhost/\n"
"             // Opens new window with URL\n"
"\n"
msgstr ""
"  kfmclient exec ftp://localhost/\n"
"             // Ivula i window entsha nge URL\n"
"\n"

msgid ""
"  kfmclient exec file:/root/Desktop/emacs.desktop\n"
"             // Starts emacs\n"
"\n"
msgstr ""
"  kfmclient exec file:/root/Desktop/emacs.desktop\n"
"             // Iqala i-emacs\n"
"\n"

msgid ""
"  kfmclient exec file:/root/Desktop/cdrom.desktop\n"
"             // Opens the CD-ROM's mount directory\n"
"\n"
msgstr ""
"  kfmclient exec file:/root/Desktop/cdrom desktop\n"
"            // Ivula ii CD-ROM unyuso lolawulo\n"
"\n"

msgid ""
"  kfmclient exec .\n"
"             // Opens the current directory. Very convenient.\n"
"\n"
msgstr ""
"  kfmclient exec .\n"
"           // Ivula ulawulo lwangoku. Iluncedo kakhulu.\n"
"\n"


