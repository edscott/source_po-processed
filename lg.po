msgstr "Terekero: "

msgid "Shell: "
msgstr "Nzivvuunuzi: "

msgid "Project: "
msgstr "Pulojekiti: "

msgid "Plan:\n"
msgstr "Entegeka:\n"

msgid "Login"
msgstr "Lyakusisitemu"

msgid "Name"
msgstr "Linnya"

msgid " TTY"
msgstr " TTY"

msgid "Idle"
msgstr "Kasirikiriro"

msgid "When"
msgstr "Ddi"

