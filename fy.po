msgstr ""
"De funksje LMM() (Least Common Multiple; is LMM: Lytste Mienskiplike "
"Mearfâld) jout it lytste mienskiplike mearfâld fan twa of mear driuwende-"
"komma-wearden"

msgid "LCM(value; value)"
msgstr "LCM(wearde;wearde)"

msgid "LCM(6;4) returns 12"
msgstr "LCM(6;4) jout 12 (12 = 2*6 en 3*4)"

msgid "LCM(1.5;2.25) returns 4.5"
msgstr "LCM(1,5;2,25) jout 4,5 (4,5 = 3*1,5 en 2*2,25)"

