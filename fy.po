msgstr "POW(wearde;wearde)"

msgid "POW(1.2;3.4) equals 1.8572"
msgstr "POW(1,2;3,4) is lyk oan 1,85873"

msgid "POW(2;3) equals 8"
msgstr "POW(2;3) is lyk oan 8"

