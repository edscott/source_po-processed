msgstr "UU"

msgid "hH"
msgstr "uU"

msgid "PH"
msgstr "PU"

msgid "pH"
msgstr "pU"

msgid "MM"
msgstr "MM"

msgid "SS"
msgstr "SS"

msgid "AMPM"
msgstr "AMPM"

msgid ""
"HH:MM:SS\n"
"pH:MM:SS AMPM"
msgstr ""
"UU:MM:SS\n"
"pU:MM:SS AMPM"

msgid "YYYY"
msgstr "JJJJ"

